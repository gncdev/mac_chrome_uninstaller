# about chrome
sudo rm -rf /Applications/Google\ Chrome.app/
sudo rm -rf ~/Applications/Chrome\ Apps.localized/
sudo rm -rf ~/Library/Application\ Support/Google/Chrome/
sudo rm -rf ~/Library/Application\ Support/CrashReporter/Google\ Chrome*
sudo rm -rf ~/Library/Caches/com.google.Chrome*
sudo rm -rf ~/Library/Saved\ Application\ State/com.google.Chrome.savedState/
sudo rm -rf ~/Library/Preferences/com.google.Chrome*
sudo rm -rf ~/Library/Preferences/Google\ Chrome*
sudo rm -rf ~/Library/Google/GoogleSoftwareUpdate/Actives/com.google.Chrome
sudo rm -rf ~/Library/Google/Google\ Chrome*

# about keystone
sudo rm ~/Library/LaunchAgents/com.google.keystone.agent.plist
sudo rm ~/Library/Preferences/com.google.Keystone.Agent.plist
sudo rm -rf ~/Library/Caches/com.google.Keystone*
sudo rm -rf ~/Library/Caches/com.google.SoftwareUpdate
sudo rm -rf ~/Library/Application\ Support/Google/RLZ
sudo rm ~/Library/LaunchAgents/com.google.keystone*
